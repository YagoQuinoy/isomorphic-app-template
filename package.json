{
  "name": "isomorphic-app-template",
  "version": "1.0.0",
  "description": "An ES6 webpack babel isomorphic app",
  "scripts": {
    "build": "npm run clean && npm run webpack",
    "clean": "rimraf ./assets/img && rimraf ./dist",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/api.bundle.js",
    "start:dev": "cross-env NODE_ENV=development concurrently --kill-others \"BABEL_ENV=server nodemon src/server/index.js --exec babel-node\" \"babel-node webpack.server.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "concurrently \"webpack --config ./webpack/browser/webpack.config.babel.js\" \"BABEL_ENV=server webpack --config ./webpack/server/webpack.config.babel.js\""
  },
  "author": "Yago Quiñoy Lobariñas",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "bunyan": "^1.8.9",
    "bunyan-format": "^0.2.1",
    "concurrently": "^3.4.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.28.0",
    "ejs": "^2.5.6",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-loader": "^1.3.3",
    "postcss-modules": "^0.6.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.0",
    "restify": "^4.3.0",
    "schema-inspector": "^1.6.8",
    "style-loader": "^0.16.0",
    "webpack": "^2.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.3",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "^2.4.2"
  }
}
