{
  "name": "isomorphic-app-template",
  "version": "1.0.0",
  "description": "An ES6 webpack babel isomorphic app",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/api.bundle.js",
    "start:dev": "cross-env NODE_ENV=development concurrently --kill-others \"nodemon --watch src/server src/server/index.js --exec babel-node --presets es2015\" \"babel-node --presets es2015 webpack.server.js\"",
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run build"
  },
  "author": "Yago Quiñoy Lobariñas",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.1",
    "bunyan": "^1.8.1",
    "ejs": "^2.5.2",
    "jquery": "^3.1.1",
    "lodash": "^4.16.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "restify": "^4.1.1",
    "schema-inspector": "^1.6.6"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.2",
    "eslint": "^3.7.1",
    "nodemon": "^1.11.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  }
}
