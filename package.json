    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "bluebird": "^3.4.6",
    "bunyan": "^1.8.1",
    "bunyan-format": "^0.2.1",
    "concurrently": "^3.1.0",
