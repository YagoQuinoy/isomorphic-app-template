{
  "name": "isomorphic-app-template",
  "version": "1.0.0",
  "description": "An ES6 webpack babel isomorphic app",
  "scripts": {
    "build": "webpack",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/api.bundle.js",
    "start:dev": "cross-env NODE_ENV=development concurrently --kill-others \"nodemon src/server/index.js --exec babel-node\" \"babel-node webpack.server.js\"",
